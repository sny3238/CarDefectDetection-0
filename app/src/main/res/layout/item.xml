<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/caridtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/returndatetxt"
        android:layout_marginBottom="6dp"
        android:includeFontPadding="false"
        android:text="대여 차량 번호 : "
        android:textColor="@android:color/black"
        android:textSize="13sp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/rentdatetext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:includeFontPadding="false"
        android:text="대여 시작 날짜"
        android:textColor="@android:color/darker_gray"
        android:textSize="13sp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/returndatetxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/rentdatetext"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="7dp"
        android:includeFontPadding="false"
        android:text="대여 종료 날짜"
        android:textColor="@android:color/darker_gray"
        android:textSize="13sp"
        tools:ignore="MissingConstraints" />

</android.support.constraint.ConstraintLayout>